{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-everyone"
		},
		"LS_Mujahed_Blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Mujahed_Blob'"
		},
		"LS_Mujahed_Gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Mujahed_Gen2'"
		},
		"LS_Mujahed_SQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_Mujahed_SQL'"
		},
		"LS_Mujahed_Gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sa10decmujahedgen2.dfs.core.windows.net/"
		},
		"LS_Mujahed_SQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "everyone.database.windows.net"
		},
		"LS_Mujahed_SQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbeveryone"
		},
		"LS_Mujahed_SQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-everyone_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Mujahed_Blob_connectionString": {
						"value": "[parameters('LS_Mujahed_Blob_connectionString')]"
					},
					"LS_Mujahed_Gen2_accountKey": {
						"value": "[parameters('LS_Mujahed_Gen2_accountKey')]"
					},
					"LS_Mujahed_SQL_password": {
						"value": "[parameters('LS_Mujahed_SQL_password')]"
					},
					"LS_Mujahed_Gen2_properties_typeProperties_url": {
						"value": "[parameters('LS_Mujahed_Gen2_properties_typeProperties_url')]"
					},
					"LS_Mujahed_SQL_properties_typeProperties_server": {
						"value": "[parameters('LS_Mujahed_SQL_properties_typeProperties_server')]"
					},
					"LS_Mujahed_SQL_properties_typeProperties_database": {
						"value": "[parameters('LS_Mujahed_SQL_properties_typeProperties_database')]"
					},
					"LS_Mujahed_SQL_properties_typeProperties_userName": {
						"value": "[parameters('LS_Mujahed_SQL_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}