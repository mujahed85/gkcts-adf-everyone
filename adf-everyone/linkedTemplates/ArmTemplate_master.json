{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-everyone"
		},
		"LS_Sujay_SQL_IDLWM_Destination_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_Sujay_SQL_IDLWM_Destination'"
		},
		"LS_Sujay_SQL_IDLWM_Source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_Sujay_SQL_IDLWM_Source'"
		},
		"LS_Sujay_SQL_IDLWM_Destination_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver4everyone.database.windows.net"
		},
		"LS_Sujay_SQL_IDLWM_Destination_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "idl-wm-destinationdb"
		},
		"LS_Sujay_SQL_IDLWM_Destination_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminuser"
		},
		"LS_Sujay_SQL_IDLWM_Source_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver4everyone.database.windows.net"
		},
		"LS_Sujay_SQL_IDLWM_Source_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "idl-wm-sourcedb"
		},
		"LS_Sujay_SQL_IDLWM_Source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-everyone_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Sujay_SQL_IDLWM_Destination_password": {
						"value": "[parameters('LS_Sujay_SQL_IDLWM_Destination_password')]"
					},
					"LS_Sujay_SQL_IDLWM_Source_password": {
						"value": "[parameters('LS_Sujay_SQL_IDLWM_Source_password')]"
					},
					"LS_Sujay_SQL_IDLWM_Destination_properties_typeProperties_server": {
						"value": "[parameters('LS_Sujay_SQL_IDLWM_Destination_properties_typeProperties_server')]"
					},
					"LS_Sujay_SQL_IDLWM_Destination_properties_typeProperties_database": {
						"value": "[parameters('LS_Sujay_SQL_IDLWM_Destination_properties_typeProperties_database')]"
					},
					"LS_Sujay_SQL_IDLWM_Destination_properties_typeProperties_userName": {
						"value": "[parameters('LS_Sujay_SQL_IDLWM_Destination_properties_typeProperties_userName')]"
					},
					"LS_Sujay_SQL_IDLWM_Source_properties_typeProperties_server": {
						"value": "[parameters('LS_Sujay_SQL_IDLWM_Source_properties_typeProperties_server')]"
					},
					"LS_Sujay_SQL_IDLWM_Source_properties_typeProperties_database": {
						"value": "[parameters('LS_Sujay_SQL_IDLWM_Source_properties_typeProperties_database')]"
					},
					"LS_Sujay_SQL_IDLWM_Source_properties_typeProperties_userName": {
						"value": "[parameters('LS_Sujay_SQL_IDLWM_Source_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}